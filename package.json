{
  "name": "shared",
  "version": "0.0.0-semantically-released",
  "description": "Shared resources library for multiple projects",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "semantic-release": "semantic-release",
    "build": "tsup src",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joedysonbezerra/shared.git"
  },
  "keywords": [],
  "author": "Jo√©dyson Bezerra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joedysonbezerra/shared/issues"
  },
  "homepage": "https://github.com/joedysonbezerra/shared#readme",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "devDependencies": {
    "@types/humps": "^2.0.3",
    "@types/node": "^18.11.9",
    "@types/uuid": "^9.0.2",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.2",
    "semantic-release": "^21.1.1",
    "tsup": "^7.0.0",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": "18.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint 'src/**' --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "humps": "^2.0.1",
    "inversify": "^6.0.1",
    "pino": "^8.15.0",
    "uuid": "^9.0.0"
  }
}
